{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "allowJs": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "allowSyntheticDefaultImports": true,
    "jsx": "react",
    "jsxFactory": "h",
    "lib": [
      "es7",
      "es2017.object",
      "dom"
    ]
  },
  "include": [
    "src/**/*"
  ],"exclude": [
        "**/*.spec.ts"
  ]
}